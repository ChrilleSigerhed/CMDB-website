@model Interaktiva20_4.Models.ViewModel.HomeViewModel

    <div class="container">
        <div class="row mx-0">
            <div class="col-md-12 col-12 paddding animate-box" data-animate-effect="fadeIn">
                <div class="fh5co_suceefh5co_height">
                    <img src=@Model.MovieList[0].img alt="img" />
                    <div class="fh5co_suceefh5co_height_position_absolute"></div>
                    <div class="fh5co_suceefh5co_height_position_absolute_font">
                        <div class="">
                            <a href="#" class="color_fff">
                                <i class="fa fa-clock-o"></i>&nbsp;&nbsp;Top rated movie right now!
                            </a>
                        </div>
                        <div class=""><a href="single.html" class="fh5co_good_font"> @Model.MovieList[0].title </a></div>
                        <form asp-action="Search" asp-controller="About" method="get">
                            <p>@Html.DisplayNameFor(x => x.Movies)</p>

                            <p>@Html.DropDownListFor(x => Model.Movies, new SelectList(Model.Movies, "Value", "Text"), "Sök film...", new { @onchange = "onChange(this.value)" })</p>
                            <br />
                            <p>@Html.DisplayNameFor(model => model.Title)</p>
                            <p><input asp-for="Title" type="text" /></p>
                            @*<p> @Html.TextBoxFor(x => x.TotalConfirmed)</p>*@
                            <p>@Html.DisplayNameFor(model => model.ImdbId)</p>
                            <p>@Html.DisplayFor(model => model.ImdbId)</p>
                            <button type="submit">OK</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
